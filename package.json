{
  "name": "@flashbacktech/flashbackclient",
  "version": "0.1.32",
  "type": "commonjs",
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc",
    "dev:watch": "nodemon --exec 'npm run dev || exit 1'",
    "lint": "eslint . --ext .ts",
    "build": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "format": "eslint \"**/*.ts\" --ignore-pattern 'dist/**' --fix && prettier --write . --ignore-path .gitignore",
    "docs": "typedoc",
    "pub": "npm run build && npm version patch && npm publish --access public"
  },
  "author": "Javier Ortiz <javier.ortiz@flashback.tech>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.777.0",
    "@google-cloud/storage": "^7.15.0",
    "@stellar/stellar-sdk": "^13.0.0",
    "formdata-node": "^6.0.3",
    "google-auth-library": "^9.15.1"
  },
  "bin": {
    "": "./dist/.min.js"
  },
  "exports": {
    "./stellar": {
      "types": "./dist/stellar/index.d.ts",
      "require": "./dist/stellar/index.js",
      "default": "./dist/stellar/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "require": "./dist/api/index.js",
      "default": "./dist/api/index.js"
    },
    "./gcs": {
      "types": "./dist/gcs/index.d.ts",
      "require": "./dist/gcs/index.js",
      "default": "./dist/gcs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@azure/storage-blob": "^12.27.0",
    "@eslint/js": "^8.56.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "typesVersions": {
    "*": {
      "stellar": [
        "./dist/stellar/index.d.ts"
      ],
      "api": [
        "./dist/api/index.d.ts"
      ],
      "gcs": [
        "./dist/gcs/index.d.ts"
      ]
    }
  }
}
