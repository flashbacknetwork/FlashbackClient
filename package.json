{
  "name": "@flashbacktech/flashonstellar",
  "version": "0.0.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc",
    "dev:watch": "nodemon --exec 'npm run dev || exit 1'",
    "lint": "eslint . --ext .ts",
    "build": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "format": "eslint \"**/*.ts\" --ignore-pattern 'dist/**' --fix && prettier --write . --ignore-path .gitignore",
    "docs": "typedoc",
    "publish": "npm run build && npm version patch && npm publish --access public"
  },
  "author": "Javier Ortiz <javier.ortiz@flashback.tech>",
  "license": "ISC",
  "dependencies": {
    "@stellar/stellar-sdk": "^13.0.0"
  },
  "bin": {
    "": "./dist/.min.js"
  },
  "exports": {
    "./stellar": {
      "types": "./dist/stellar/index.d.ts",
      "import": "./dist/stellar/index.js",
      "default": "./dist/stellar/index.js"
    },
    "./path2": {
      "types": "./dist/path2/index.d.ts",
      "import": "./dist/path2/index.js",
      "default": "./dist/path2/index.js"
    },
    "./path3": {
      "types": "./dist/path3/index.d.ts",
      "import": "./dist/path3/index.js",
      "default": "./dist/path3/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "type": "module",
  "typesVersions": {
    "*": {
      "stellar": [
        "./dist/stellar/index.d.ts"
      ],
      "path2": [
        "./dist/path2/index.d.ts"
      ],
      "path3": [
        "./dist/path3/index.d.ts"
      ]
    }
  }
}
